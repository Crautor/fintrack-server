datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native", "windows", "debian-openssl-3.0.x"]
}

model PaymentMethod {
  paymentMethodId Int      @id @default(autoincrement())
  name            String
}
